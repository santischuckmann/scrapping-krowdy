{
    "manifest_version": 3,
    "name": "scrapper",
    "version": "0.0.1",
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_title": "Click to show an alert"
    },
    "permissions": ["tabs", "scripting", "activeTab"],
    "host_permissions": [
        "https://www.linkedin.com/*" 
    ]
}